{
    "server" : {
        "protocol" : ["http","https"]
    },
    "mongodb" : {
        "options" : {
            "useNewUrlParser": true,
            "bufferCommands"    : true,
            "autoIndex"         : true,
            "autoReconnect"     : true,
            "reconnectInterval" : 2000,
            "poolSize"          : 10,
            "socketTimeoutMS"   : 15000
        } 
    }
}